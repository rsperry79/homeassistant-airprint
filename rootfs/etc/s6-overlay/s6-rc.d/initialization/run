#!/command/with-contend bashio
mkdir -p /var/run/dbus

bashio::log.info "Initializing Common Config"
bashio /etc/s6-overlay/s6-rc.d/initialization/common

bashio::log.debug "Initializing Cups files"
bashio /etc/s6-overlay/s6-rc.d/initialization/cups-files

bashio::log.debug "Initializing Avahi files"
bashio /etc/s6-overlay/s6-rc.d/initialization/avahi-files

bashio::log.debug "Running Additional packages"
bashio /etc/s6-overlay/s6-rc.d/initialization/packages

bashio::log.debug "Updating Users"
bashio /etc/s6-overlay/s6-rc.d/initialization/users
